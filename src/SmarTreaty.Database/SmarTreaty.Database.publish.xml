<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>smartreatydb</TargetDatabaseName>
    <DeployScriptFileName>SmarTreaty.Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=smartreatydbserver.database.windows.net;Persist Security Info=True;User ID=STAdmin;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=True;TrustServerCertificate=False</TargetConnectionString>
